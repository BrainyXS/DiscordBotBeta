<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="DiscordBot.DataAccess"
                   namespace="DiscordBot.DataAccess.Entities">
    <class name="PlaylistEntity" table="playlist">
        <id name="PlaylistId" column="playlistId" type="Int64">
            <generator class="increment"/>
        </id>
        <property name="UserId" type="String" column="userId"/>
        <property name="Title" type="String" column="title"/>
    </class>
</hibernate-mapping>