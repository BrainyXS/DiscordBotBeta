<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="DiscordBot.DataAccess"
                   namespace="DiscordBot.DataAccess.Entities">
    <class name="GuildConfigEntity" table="guildConfiguration">
        <id name="Id" column="configId" type="Int64">
            <generator class="increment"/>
        </id>
        <property name="GuildId" type="String" column="guildId"/>
        <property name="Prefix" type="String" column="prefix"/>
    </class>
</hibernate-mapping>
